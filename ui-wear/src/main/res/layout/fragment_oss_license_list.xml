    android:layout_height="match_parent"
    android:padding="@dimen/oss_licenses_inner_frame_layout_padding"
    app:boxedEdges="all"
    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

</androidx.wear.widget.BoxInsetLayout>
